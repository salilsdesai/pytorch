
{
    "problemMatcher": [
        {
            "owner": "eslint-stylish",
            "pattern": [
                {
                    "regexp": "{\"file\":\"(.*)\",\"line\":(\\d+),\"column\":(\\d+),\"severity\":\"(.*)\",\"code\":\"(.*)\",\"message\":\"(.*)\"}",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "code": 5,
                    "message": 6
                }
            ]
        }
    ]
}
